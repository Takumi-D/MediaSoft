{
  "compilerOptions": {
    "outDir": "",
    "noImplicitAny": false,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./src",
    "paths": {
      "@store/*": ["./store/*"],
      "@type/*": ["./type/*"],
      "@images/*": ["./images/*"],
      "react": [ "./node_modules/@types/react" ],
    }
  }
}